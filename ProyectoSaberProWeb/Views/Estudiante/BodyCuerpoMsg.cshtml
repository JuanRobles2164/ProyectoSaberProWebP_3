
@{
    ViewBag.Title = "BodyCuerpoMsg";
}

<h2>Puntuaci&oacute;n Obtenida #Nombre de la prueba#</h2>
<div bgcolor="#ffffff" style="font-family:Roboto,Helvetica,Arial,sans-serif;margin:0;padding:0">

    <table bgcolor="#fafafa" border="0" cellpadding="0" cellspacing="0" role="presentation" style="border:1px solid #f0f0f0;border-bottom:1px solid #c0c0c0;border-bottom-left-radius:3px;border-bottom-right-radius:3px;border-top:0" width="100%">
        <tbody>
            <tr>
                <td rowspan="3" width="24px"></td>
                <td colspan="2" height="24px"></td>
                <td rowspan="3" width="24px"></td>
            </tr>
            <tr>
                <td style="padding-right:24px" valign="top" width="72px">
                    <div aria-hidden="true" style="border-radius:50%;background-color:#0BDB65;height:72px;width:72px">
                        <table border="0" cellpadding="0" cellspacing="0" height="72px" role="presentation" style="color:#ffffff;font-size:16px;font-weight:400;text-align:center" width="72px">
                            <tbody>
                                <tr>
                                    <td rowspan="2" width="16px"></td>
                                    <td style="border-bottom:1px solid #ffffff;padding-bottom:4px;vertical-align:bottom"><!--Aqui va la puntuacion que saco -->#PuntuacionSacada#</td>
                                    <td rowspan="2" width="16px"></td>
                                </tr>
                                <tr>
                                    <td style="padding-top:4px;vertical-align:top"><!--sobre el numero de preguntas que eran(si no al contrario)-->#PuntuacionTotal#</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <img aria-label="5 puntos de 5" height="1" width="1" style="height:1px;width:1px;opacity:0.0001" class="CToWUd">
                </td>
                <td>
                    <table cellpadding="0" cellspacing="0" role="presentation">
                        <tbody>
                            <tr>
                                <td style="color:#212121;font-size:14px;font-weight:400"></td>
                            </tr>
                            <tr height="0"></tr>
                            <tr>
                                <td style="color:#212121;font-size:20px;font-weight:500"> Ya puede ver los resultados de la prueba!</td>
                            </tr>
                            <tr height="0"></tr>
                            <tr>
                                <td style="color:#737373;font-size:14px;font-weight:400"></td>
                            </tr>
                            <tr height="16px"></tr>
                            <tr>
                                <td>
                                    <table border="0" cellpadding="0" cellspacing="0" role="presentation" style="border-collapse:collapse;display:inline-block">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    Su puntuaci&oacute;n total fue de:
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td colspan="2" height="24px"></td>
            </tr>
        </tbody>
    </table>

</div>

